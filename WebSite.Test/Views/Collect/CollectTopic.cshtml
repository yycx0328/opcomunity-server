
@{
    ViewBag.Title = "收藏作品"; 
}
 
<h2 style="text-align: center;">收藏作品</h2>
<form class="form-horizontal" action="/Collect/CollectTopic" method="post">
    <div class="form-group">
        <label for="txtUserId" class="col-sm-1 control-label">userid</label>
        <div class="col-sm-10">
            <input name="userId" type="text" class="form-control" id="txtUserId" placeholder="收藏者用户Id">
        </div>
    </div>
    <div class="form-group">
        <label for="txtTopicId" class="col-sm-1 control-label">topicid</label>
        <div class="col-sm-10">
            <input name="topicId" type="text" class="form-control" id="txtTopicId" placeholder="收藏的作品Id">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <button type="submit" class="btn btn-primary">提交</button>
        </div>
    </div>
    <div class="form-group">
        <label for="txtResult" class="col-sm-1 control-label">Result</label>
        <div class="col-sm-10">
            <textarea class="form-control" id="txtResult" rows="5">@ViewData["Result"]</textarea>
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <p><a href="~/Default/Index" class="btn btn-default" style="text-align:left" role="button">返回接口列表</a></p>
        </div>
    </div>
</form>